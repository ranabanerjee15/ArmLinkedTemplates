{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "VnetName": {
            "type": "string",
            "defaultValue": "Hub-Vnet",
            "metadata": {
                "description": "Name of the Vnet"
            }
        }
    },
    "functions": [],
    "variables": {
        "Template-Vnet": "/artifacts/Vnet.json"
    },
    "resources": [
        {
            "name": "Vnet",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat('artifactsLocation', '//linkedTemplate.json', 'artifactsLocationSasToken')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                }
            }
        }
    ],
    "outputs": {}
}